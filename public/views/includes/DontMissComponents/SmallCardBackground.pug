each data in scwB
    .card.card_default.card_small_with_background.grid-item
        .card_background(style="background-image:url("+data.postImageSrc+")")
            .card_mask
        .card-body
            .card-title.card-title-small
                a(href=''+data.postLink+'')
                    | #{data.postTitle}
            small.post_meta
                a(href='#') #{data.postSource}
                span Sep 29, 2017 at 9:48 am